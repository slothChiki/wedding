{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "esnext",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "strict": true,
    "noEmit": false,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "preserveConstEnums": true,
    "allowJs": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "modules/reducers/dummy/main.ts",
    "next.config.ss",
    ".eslintrc.js",
    "pm2_*.config.js",
    "src/common/kakao/kakao-navi.js"
  ],
  "exclude": [
    "node_modules",
    "test",
    "dist",
    "**/*spec.ts",
    ".next"
  ],
  "declaration": true,
  "emitDecoratorMetadata": true,
  "target": "es2017",
  "incremental": true,
  "noImplicitAny": false,
  "strictBindCallApply": false
}
